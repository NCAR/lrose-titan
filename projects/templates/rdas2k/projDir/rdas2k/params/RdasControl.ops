        <label>Antenna El Correction</label>
        <description>Correction for pedestal antenna position in elevation (decimal deg)</description>
        <info>
          <![CDATA[
          This value is added to the pedestal readout to correct the elevation value.
          ]]>
        </info>
        <value>0.0</value>
      </param>

      <param name="antennaAzCorr" type="Double">
        <label>Antenna Az Correction</label>
        <description>Correction for pedestal antenna position in azimuth (decimal deg)</description>
        <info>
          <![CDATA[
          This value is added to the pedestal readout to correct the azimuth value.
          ]]>
        </info>
        <value>0.0</value>
      </param>

      <param name="controlElCorr" type="Double">
        <label>Control El Correction</label>
        <description>Correction for driving antenna in elevation (decimal deg)</description>
        <info>
          <![CDATA[
          If the antenna elevation readout does not match the intended value, correct it by inserting a correction value here.
          ]]>
        </info>
        <value>0.0</value>
      </param>

      <param name="controlAzCorr" type="Double">
        <label>Control Az Correction</label>
        <description>Correction for driving antenna in azimuth (decimal deg)</description>
        <info>
          <![CDATA[
          If the antenna azimuth readout does not match the intended value, correct it by inserting a correction value here.
          ]]>
        </info>
        <value>0.0</value>
      </param>

      <param name="antennaMinElev" type="Double">
        <label>Antenna min el</label>
        <description>Min antenna elevation (deg)</description>
        <info>
          <![CDATA[
          The antenna cannot go below this elevation. Therefore the control program will not request elevation angles below this value.
          ]]>
        </info>
        <value>0.0</value>
      </param>

      <param name="antennaMaxElev" type="Double">
        <label>Antenna max el</label>
        <description>Max antenna elevation (deg)</description>
        <info>
          <![CDATA[
          The antenna cannot go above this elevation. Therefore the control program will not request elevation angles above this value.
          ]]>
        </info>
        <value>55.0</value>
      </param>

      <param name="antennaMaxElSlewRate" type="Double">
        <label>Antenna max el slew rate</label>
        <description>Max el slew rate (deg/s)</description>
        <info>
          <![CDATA[
