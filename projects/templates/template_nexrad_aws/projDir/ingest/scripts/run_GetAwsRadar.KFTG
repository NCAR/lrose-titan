#! /bin/csh

# get the data from the KFTG radar, using AWS

cd $PROJ_DIR/ingest/scripts

running "GetAwsRadar.py --radarName KFTG"

if ($status == 1) then
  GetAwsRadar.py --radarName KFTG --debug \
    --realtime \
    --lookbackSecs 3600 \
    --sleepSecs 15 \
      |& LogFilter -d $ERRORS_LOG_DIR -p GetAwsRadar -i KFTG \
      >& /dev/null &
endif

